{
    "name": "remithomas/rt-headtitle",
    "description": "ZF2 Plugin to set title easily to your pages from controllers actions",
    "type": "library",
    "keywords": [
        "zf2","title","headtitle"
    ],
    "homepage": "https://github.com/remithomas/rt-headtitle",
    "authors": [
        {
            "name": "Remi THOMAS",
            "email": "remi.thomas@yahoo.fr",
            "homepage": "http://remithomas.fr/"
        }
    ],
    "version": "0.1",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-eventmanager": "2.*",
        "zendframework/zend-modulemanager": "2.*",
        "zendframework/zend-mvc": "2.*"
    },
    "autoload": {
        "psr-0": {
            "RtHeadtitle": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
